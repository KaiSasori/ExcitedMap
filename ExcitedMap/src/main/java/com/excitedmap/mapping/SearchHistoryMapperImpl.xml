<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.excitedmap.dao.SearchHistoryMapperImpl">
	<resultMap id="BaseResultMap" type="SearchHistory"
		extends="com.excitedmap.dao.SearchHistoryMapper.BaseResultMap">
	</resultMap>
	<select id="selectByUserIdAndKeywordWithLimit" resultMap="BaseResultMap">
		select *
		from search_history
		where user_id = #{userId} and search_text like '%${keyword}%' limit
		#{limit};
	</select>
</mapper>